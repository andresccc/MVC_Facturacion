@model IEnumerable<Facturacion_MVC.Models.TBLEMPLEADO>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutFacturacion.cshtml";
}

<h2>ADMIN EMPLEADOS</h2>
<div class="well">
    <a href="~/TBLempleado/Nuevo" class="btn btn-success">Nuevo empleado</a>
</div>

<table class="table">
    <tr>
        <th>Nombre</th>
        <th>Documento</th>
        <th>Telefono</th>
        <th>Correo</th>
        <th>ACCIONES</th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>@item.strNombre</td>
        <td>@item.NumDocumento</td>
        <td>@item.StrTelefono</td>
        <td>@item.StrEmail</td>
        <td><a href="#" class="btn btn-info">Editar</a>  <a href="#" class="btn btn-danger">Borrar</a></td>
    </tr>
    }
</table>
